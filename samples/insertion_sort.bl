insertion_sort = function(arr) {
    i = 1;
    n = len(arr);

    while (i < n) {
        key = arr[i];
        before = i - 1;

        while (before >= 0 && arr[before] > key) {
            arr[before + 1] = arr[before];
            before = before - 1;
        }

        arr[before + 1] = key;
        i = i + 1;
    }

    return arr;
};

my_array = [5, 2, 9, 1, 5, 6];
println(insertion_sort(my_array));
